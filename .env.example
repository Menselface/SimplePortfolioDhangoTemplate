# Database Configuration
ENGINE=django.db.backends.postgresql       # Example: django.db.backends.postgresql
NAME=example_db                            # Database name
USER=example_user                          # Database user
PASSWORD=example_password                  # Database password
HOST=127.0.0.1                             # Default for local development
PORT=5432                                  # Default PostgreSQL port

# Cloudinary Keys
CLOUD_NAME=your_cloud_name                 # Cloudinary account name
API_KEY=your_api_key                        # Cloudinary API key
API_SECRET=your_api_secret                  # Cloudinary API secret

# SMTP Settings
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.example.com                 # SMTP server address
EMAIL_PORT=587                              # Port for TLS
EMAIL_USE_TLS=True                          # Use TLS for email
EMAIL_HOST_USER=example@example.com         # Your email address
EMAIL_HOST_PASSWORD=example_password        # Your email password
DEFAULT_FROM_EMAIL=example@example.com      # Default email sender

# Django Settings
SECRET_KEY=django-insecure-examplekey       # Replace with a secure key
ALLOWED_HOSTS=localhost,127.0.0.1           # Allowed hosts, use '*' for testing only!

# Contact Email
MY_MAIL=example@example.com                 # Email for receiving messages
